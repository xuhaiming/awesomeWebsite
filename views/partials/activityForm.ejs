<h1><%= title %></h1>
<form id="activityForm" action="/activities" method="POST">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" type="text" name="title" />
  </div>
  <div class="form-group">
    <label for="imageUrl">Image Url</label>
    <input class="form-control" type="text" name="imageUrl" />
  </div>
  <div class="form-group">
    <label for="tags">Tags</label>
    <input class="form-control" type="text" name="tags" />
  </div>
  <div class="form-group">
    <label for="location">Location</label>
    <input class="form-control" type="text" name="location" />
  </div>
  <div class="form-group">
    <label for="eventDate">Event Date</label>
    <input class="form-control" type="date" name="eventDate" />
  </div>
  <div clas="form-group">
    <label for="content">Content</label>
    <textarea clas="form-control" name="content"></textarea>
  </div>
  <input class="btn btn-primary activitySubmitBtn" type="submit" value="Create" />

</form>

